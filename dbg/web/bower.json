{
  "dependencies": {
    "jquery": "2.1.3",
	"angular": "1.3.15",
	"underscore": "1.7.0",
	"underscore.string": "2.3.3"
  },
  "deploy": {
    "jquery": {
      "files": [
        {
          "src": "jquery-2.1.3/dist/jquery.js",
          "dest": [
            "js/lib/jquery.js"
          ]
        }
      ]
    },
    "angular": {
      "files": [
        {
          "src": "angular-1.3.15/angular.js",
          "dest": [
            "js/lib/angular.js"
          ]
        }
      ]
    },
    "underscore": {
      "files": [
        {
          "src": "underscore-1.7.0/underscore.js",
          "dest": [
            "js/lib/underscore.js"
          ]
        }
      ]
    },
    "underscore.string": {
      "files": [
        {
          "src": "underscore.string-2.3.3/lib/underscore.string.js",
          "dest": [
            "js/lib/underscore.string.js"
          ]
        }
      ]
    }
  }
}
